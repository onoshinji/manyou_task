<div class = "text-center">
  <h2 >ログインフォーム</h2>
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <%= f.label :メールアドレス %>
    <%= f.email_field :email %>
    <br>
    <%= f.label :パスワード %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit "ログイン" %>
  <% end %>
</div>
